<Window x:Class="GemLogAnalyzer.Views.TitleWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GemLogAnalyzer.Views"
        xmlns:vm="clr-namespace:GemLogAnalyzer.ViewModels"
        xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
        mc:Ignorable="d"
        Title="TitleWindow" Height="500" Width="500" WindowStyle="None" WindowStartupLocation="CenterScreen">
    <Window.DataContext>
        <vm:TitleWindowViewModel/>
    </Window.DataContext>
    <Window.Background>
        <ImageBrush/>
    </Window.Background>
    <Grid>
        <Image Source="/Resource/Title.png" Stretch="Fill"/>
    </Grid>
</Window>
